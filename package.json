{
  "name": "vscode-file-properties",
  "displayName": "File Properties",
  "description": "Show the current file properties in the status bar",
  "version": "1.0.0",
  "publisher": "gaojr",
  "repository": {
    "type": "git",
    "url": "https://github.com/gaojr/vscode-file-properties"
  },
  "icon": "icon.png",
  "license": "MIT",
  "engines": {
    "vscode": "^1.54.0"
  },
  "keywords": [
    "file",
    "properties",
    "fileproperties",
    "status bar"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown",
    "onLanguage:plaintext"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^18.14.5",
    "@types/vscode": "^1.54.0",
    "@vscode/vsce": "2.18.0",
    "typescript": "^4.2.3"
  }
}
